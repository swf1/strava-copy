# platform :ios, '9.0'

target 'actio' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for actio
  pod 'FacebookLogin'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'GoogleSignIn'
  pod 'FBSDKLoginKit'
  pod 'FBSDKCoreKit'
  pod 'Bolts'
  pod 'Mapbox-iOS-SDK', '~> 4.0.0' # 3.7(?) will be installed without specific version
  pod 'Repeat'
  
  
  target 'actioTests' do

    inherit! :search_paths
    # Pods for testing
end

end
